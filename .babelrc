{
  "presets": [
    "es2015"
  ],

  "plugins": [
    "syntax-async-functions",
    "syntax-export-extensions",

    ["transform-builtin-extend", {
      "globals": ["Error"]
    }],

    "transform-export-extensions",
    "transform-regenerator",

    ["webpack-alias", {
      "config": ".alias.js",
      "findConfig": true
    }]
  ],

  "sourceMaps": "inline",

  "comments": true
}
